n        alert(3); //此处写逻辑\r\n      }\r\n    };\r\n\r\n\r\n\r\n    return {\r\n       titles,\r\n        detailClick,\r\n        handleClick,\r\n    }\r\n\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n\r\n.r-list {\r\n  font-family: \"Noto Serif CJK SC\", \"Noto Serif CJK\", \"Source Han Serif SC\",\r\n    ‘Source Han Serif’, source-han-serif-sc, serif;\r\n  text-align: center;\r\n  display: flex;\r\n  flex-flow: row wrap;\r\n  justify-content: space-evenly;\r\n\r\n  dl {\r\n    background: url(\"../../../assets/img/content/边框.png\") no-repeat;\r\n    width: 420px;\r\n    height: 400px;\r\n    dt img {\r\n      width: 410px;\r\n      height: 300px;\r\n      padding-top: 20px;\r\n    }\r\n    dd {\r\n      h3 {\r\n        text-align: left;\r\n        white-space: nowrap; /*超出的空白区域不换行*/\r\n        overflow: hidden; /*超出隐藏*/\r\n        text-overflow: ellipsis; /*文本超出显示省略号*/\r\n\r\n        font-size: 14px;\r\n        font-weight: normal;\r\n        &::after {\r\n          content: \"\";\r\n          width: 50px;\r\n          height: 2px;\r\n          background: #999;\r\n          display: block;\r\n          margin-top: 5px;\r\n        }\r\n      }\r\n      p {\r\n        font-size: 16px;\r\n        font-weight: bold;\r\n        text-align: left;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"]}]}